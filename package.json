{
  "name": "Mentoring",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Jeffzholy/Mentoring.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "yarn build-frontend && yarn build-backend",
    "build-backend": "babel backend/server.js -d dist/backend",
    "start-backend": "node dist/backend/server.js",
    "dev-backend": "nodemon -r @babel/register backend/server.js",
    "build-frontend": "webpack --config webpack.config.prod.js",
    "dev-frontend": "webpack-dev-server --config webpack.config.dev.js --open",
    "newman": "newman run collection.json --delay-request 5000"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "style-loader": "^1.1.1",
    "supertest": "^4.0.2",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "newman": "^4.5.7",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          }
        }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      "emotion"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "node"
  }
}